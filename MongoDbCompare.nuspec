<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.7.2">
    <id>JimBobBennett.MongoDbCompare</id>
    <version>$version$</version>
    <title>MongoDb collection comparison tool</title>
    <authors>Jim Bennett</authors>
    <owners>Jim Bennett</owners>
    <licenseUrl>https://github.com/jimbobbennett/MongoDBCompare/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jimbobbennett/MongoDBCompare</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A .Net tool to compare 2 MongoDB collections.
    </description>
    <summary>A .Net tool to compare 2 MongoDB collections.</summary>
    <tags>C# .net MongoDB comparison</tags>
    <dependencies>
      <dependency id="MongoDB.Bson" version="2.0.1" />
      <dependency id="MongoDB.Driver" version="2.0.1" />
      <dependency id="MongoDB.Driver.Core" version="2.0.1" />
    </dependencies>
  </metadata>
  <files>
    <file src=".\MongoDbCompare\bin\$Configuration$\MongoDbCompare.dll" target="lib\net45\MongoDbCompare.dll" />
    <file src=".\MongoDbCompare\bin\$Configuration$\MongoDbCompare.pdb" target="lib\net45\MongoDbCompare.pdb" />
    <file src=".\**\*.cs" target="src\" />
  </files>
</package>
